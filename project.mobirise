          "_name": "pricing-table2",
          "_customHTML": "<section class=\"pricing-table2\" group=\"Pricing Tables\" data-bg-video=\"{{bg.type == 'video' &amp;&amp; bg.value.url}}\" mbr-class=\"{'mbr-parallax-background':bg.parallax}\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input inline=\"\" type=\"range\" title=\"Arriba\" name=\"paddingTop\" min=\"0\" max=\"8\" step=\"1\" value=\"6\">\n        <input inline=\"\" type=\"range\" title=\"Abajo\" name=\"paddingBottom\" min=\"0\" max=\"8\" step=\"1\" value=\"6\">\n\n        <input type=\"checkbox\" title=\"Mostrar Título\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Mostrar Subtítulo\" name=\"showSubtitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Mostrar Subrayado\" name=\"showUnderline\" checked=\"\" condition=\"showSubtitle\">\n        <input type=\"color\" title=\"Color del Subrayado\" name=\"underlineColor\" value=\"#ffffff\" condition=\"showSubtitle &amp;&amp; showUnderline\">\n        <input type=\"checkbox\" title=\"Mostrar Texto\" name=\"showText\">\n        <input type=\"range\" inline=\"\" title=\"Nº Tarjetas\" name=\"cardsCount\" min=\"1\" max=\"4\" step=\"1\" value=\"4\">\n        <input inline=\"\" type=\"range\" title=\"1ª Estrella de Tarjeta\" name=\"card1Stars\" min=\"1\" max=\"5\" step=\"1\" value=\"2\">\n        <input inline=\"\" type=\"range\" title=\"2ª Estrella de Tarjeta\" name=\"card2Stars\" min=\"1\" max=\"5\" step=\"1\" value=\"3\" condition=\"cardsCount >= 2\">\n        <input inline=\"\" type=\"range\" title=\"3ª Estrella de Tarjeta\" name=\"card3Stars\" min=\"1\" max=\"5\" step=\"1\" value=\"4\" condition=\"cardsCount >= 3\">\n        <input inline=\"\" type=\"range\" title=\"4ª Estrella de Tarjeta\" name=\"card4Stars\" min=\"1\" max=\"5\" step=\"1\" value=\"5\" condition=\"cardsCount >= 4\">\n        <input type=\"color\" title=\"Color de las Estrellas\" name=\"starsColor\" value=\"#ffffff\">\n\n        <input type=\"checkbox\" title=\"Mostrar Iconos\" name=\"showIcons\" checked=\"\">\n        <input type=\"checkbox\" title=\"Mostrar Botones en Tarjeta\" name=\"showCardButtons\" checked=\"\">\n        <input type=\"color\" title=\"Color BG de la Tarjeta\" name=\"cardColor\" value=\"#ffffff\">\n        <input type=\"color\" title=\"Color BG Encabezamiento de Tarjeta\" name=\"cardHeadColor\" value=\"#c19b76\">\n        \n        <fieldset type=\"background\" name=\"bg\" parallax=\"\">\n            <input type=\"image\" title=\"Imagen de Fondo\" value=\"../_images/background7.jpg\" selected=\"\">\n            <input type=\"color\" title=\"Color de Fondo\" value=\"#cccccc\">\n            <input type=\"video\" title=\"Vídeo de Fondo\" value=\"https://www.youtube.com/watch?v=2jqUN6d6ZyM\">\n        </fieldset>\n\n        <input type=\"checkbox\" title=\"Superposición\" name=\"overlay\" checked=\"\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Color de Superposición\" name=\"overlayColor\" value=\"#000000\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacidad\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div class=\"container\">\n        <h3 class=\"mbr-section-subtitle mbr-fonts-style mbr-white mbr-lighter align-center\" mbr-class=\"{'pb-3': !showUnderline}\" mbr-theme-style=\"display-4\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle, .underline\">\n            HOTEL LUXURY\n        </h3>\n\n        <div class=\"underline align-center pb-3\" mbr-if=\"showUnderline &amp;&amp; showSubtitle\">\n            <div class=\"line\"></div>\n        </div>\n    \n        <h2 cl